{
  "name": "farm-activity-entry",
  "version": 2,
  "alias": ["farm-entry"],
  "env": {
    "NAV_BASE_URL": "@nav-base-url",
    "MONGO_URI": "@farm-activity-mongo-uri",
    "SESSION_SECRET": "@farm-activity-session-secret"
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "web/dist" } },
    { "src": "api/index.ts", "use": "@now/node" }
  ],
  "routes": [
     {
        "src": "/api/(.*)",
        "dest": "/api/index.ts"
     },
     {
        "src": "/static/(.*)",
        "headers": {
           "cache-control": "s-maxage=31536000, immutable"
        },
        "dest": "/static/$1"
     },
     {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
     },
     {
        "src": "/logo192.png",
        "dest": "/logo192.png"
     },
     {
        "src": "/logo512.png",
        "dest": "/logo512.png"
     },
     {
        "src": "/asset-manifest.json",
        "dest": "/asset-manifest.json"
     },
     {
        "src": "/manifest.json",
        "dest": "/manifest.json"
     },
     {
        "src": "/precache-manifest.(.*)",
        "dest": "/precache-manifest.$1"
     },
     {
        "src": "/service-worker.js",
        "headers": {
           "cache-control": "s-maxage=0"
        },
        "dest": "/service-worker.js"
     },
     {
        "src": "/sockjs-node/(.*)",
        "dest": "/sockjs-node/$1"
     },
     {
        "src": "/(.*)",
        "headers": {
           "cache-control": "s-maxage=0"
        },
        "dest": "/index.html"
     }
  ]
}