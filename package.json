    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "html-webpack-plugin": "4.0.0-beta.5",
    "httpntlm": "^1.7.6",
    "interpolate-html-plugin": "3.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "mongoose": "5.7.3",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.18.2",
