    "react-dev-utils": "9.1.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.1.2",
    "typescript": "3.5.3",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-manifest-plugin": "2.2.0"
  },
  "scripts": {
    "start:web": "NODE_ENV=development webpack-dev-server --config ./web/config/webpack.config.js",
    "start:api": "nodemon --watch api --ext .ts,.js --exec 'ts-node --project api/tsconfig.json api/index.ts'",
    "build": "NODE_ENV=production webpack --config ./web/config/webpack.config.js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "e2e": "cypress open"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "4.1.0",
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "9.1.1",
    "@types/body-parser": "^1.17.1",
    "@types/connect-mongo": "0.0.43",
    "@types/dotenv": "^6.1.1",
    "@types/express-session": "^1.15.14",
    "@types/faker": "4.1.5",
    "@types/fetch-mock": "7.3.1",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.1",
    "@types/passport": "^1.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "4.3.4",
    "cypress": "3.4.1",
    "faker": "4.1.0",
    "fetch-mock": "7.3.9",
    "husky": "3.0.3",
    "lint-staged": "9.2.1",
    "node-fetch": "2.6.0",
    "nodemon": "^1.19.1",
    "ts-node": "8.4.1",
    "webpack-dev-server": "3.8.2"
  }
}
