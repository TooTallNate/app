{
  "name": "farm-activity-entry",
  "version": "0.4.2",
  "private": true,
  "scripts": {
    "web": "NODE_ENV=development webpack-dev-server --config ./web/webpack.config.js",
    "api": "NODE_ENV=development nodemon --watch api --ext .ts,.js --exec 'ts-node --project api/tsconfig.json api/src/index.ts'",
    "build": "NODE_ENV=production webpack --config ./web/webpack.config.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "cypress open",
    "generate": "graphql-codegen",
    "preversion": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
